{
  "name": "deepdive",
  "version": "9.1.7",
  "description": "Why DeepDive ðŸŒŠ Because the format is liquid ðŸ’¦ ðŸ˜‰",
  "scripts": {
    "clean": "rm -f assets/js/*{.min.js,.map,.LICENSE.txt} && rm -f assets/js/legacy/*{.min.js,.map,.LICENSE.txt}",
    "test": "exit 0",
    "format": "prettier --write \"_js/src/**/*.js\"",
    "build": "npm run clean && npm run build:js & npm run build:css & npm run build:scripts & wait",
    "build:js": "webpack --mode production --color",
    "build:js:stats": "webpack --mode production --progress --color --json > stats.json",
    "build:css": ".scripts/build-css.js",
    "build:scripts": "for f in $(find _includes/scripts -type f ! -name '*.min.js'); do g=${f%.js}; uglifyjs $f -c -m > $g.min.js; done",
    "watch": "npm run clean && npm run watch:js & npm run watch:css",
    "watch:js": "webpack --watch --mode development --progress --color",
    "watch:css": "npm run build:css && onchange '_sass/**/*.scss' -e '_sass/**/__*/*' -- npm run build:css '{{changed}}'",
    "dev": "npm run watch",
    "preversion": "npm run preversion:js && npm run preversion:css",
    "preversion:js": "sed -i '' 's:/assets/js/\\*deepdive-\\*:#<removed/js>:' .gitignore",
    "preversion:css": "sed -i '' 's:/_sass/\\*/__\\*__/\\*:#<removed/css>:' .gitignore",
    "version": ".scripts/version.js && npm run format && npm run build && git add .",
    "postversion": "(npm run postversion:js && npm run postversion:css) && git add . && git ci -m 'Restore pre-version .gitignore' && npm run postversion:forward",
    "postversion:forward": "git branch -f v9 v$npm_package_version",
    "postversion:js": "sed -i '' 's:#<removed/js>:/assets/js/*deepdive-*:' .gitignore && git rm --cached assets/js/*deepdive-*",
    "postversion:css": "sed -i '' 's:#<removed/css>:/_sass/*/__*__/*:' .gitignore && git rm --cached _sass/*/__*__/*",
    "yarn-update": "yarn set version stable && yarn install && yarn up && yarn npm audit && npx depcheck --detailed && yarn upgrade-interactive"
  },
  "config": {
    "port": 4000
  },
  "author": "JV conseil <contact@jv-conseil.dev> (https://www.jv-conseil.dev)",
  "license": "EUPL-1.2",
  "private": true,
  "dependencies": {
    "@fontsource/noto-sans": "^5.0.18",
    "@fontsource/roboto-slab": "^5.0.18",
    "@hydecorp/component": "^1.0.0",
    "@hydecorp/drawer": "^1.0.0",
    "@hydecorp/push-state": "^1.0.0",
    "@juggle/resize-observer": "^3.4.0",
    "@webcomponents/custom-elements": "^1.6.0",
    "@webcomponents/shadycss": "^1.11.2",
    "@webcomponents/shadydom": "^1.11.0",
    "@webcomponents/template": "^1.5.1",
    "@webcomponents/url": "^0.7.9",
    "@webcomponents/webcomponents-platform": "^1.0.1",
    "abortcontroller-polyfill": "^1.7.5",
    "bootstrap": "^5.3.3",
    "broadcastchannel-polyfill": "^1.0.1",
    "core-js": "^3.36.0",
    "create-element-x": "^1.2.0",
    "intersection-observer": "^0.12.2",
    "kv-storage-polyfill": "^2.0.0",
    "lit": "^3.1.2",
    "minisearch": "^6.3.0",
    "npm": "^10.5.0",
    "rxjs": "^7.8.1",
    "scroll-into-view-if-needed": "^3.1.0",
    "tippy.js": "^6.3.7",
    "web-animations-js": "^2.3.2",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/preset-env": "^7.24.0",
    "babel-loader": "^9.1.3",
    "bower": "^1.8.14",
    "dedent": "^1.5.1",
    "depcheck": "^1.4.7",
    "eslint-config-prettier": "^9.1.0",
    "null-loader": "^4.0.1",
    "onchange": "^7.1.0",
    "prettier": "^3.2.5",
    "uglify-js": "^3.17.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0",
    "yargs": "^17.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JV-conseil/jekyll-theme-deepdive.git"
  },
  "bugs": {
    "url": "https://github.com/JV-conseil/jekyll-theme-deepdive/issues"
  },
  "homepage": "https://www.jv-conseil.dev/",
  "engines": {
    "node": ">=11"
  },
  "packageManager": "yarn@4.1.0"
}